    "def get_predict(h):\n",
    "    pred = {}\n",
    "    for w,n, in h.items():\n",
    "        for i in range(1,len(w)):\n",
    "            a = w[:i]\n",
    "            if n > h.get(pred.get(a,None),0):\n",
    "                pred[a] = w\n",
    "    return pred\n",
    "                \n",
    "h = {\"ez\": 30, \"erre\": 1, \"eraman\": 8}\n",
    "get_predict(h)    "
   ]
